# Configurações do PatentScope Scraper

# Ambiente (development, testing, production)
SCRAPER_ENV=production

# Rate Limiting
MIN_DELAY=1.0
MAX_DELAY=3.0

# Configurações de logging
LOG_LEVEL=INFO

# API Keys para LLM (opcional - para geração de termos)
ANTHROPIC_API_KEY=

# WIPO PatentScope Credentials (opcional - funciona sem autenticação)
WIPO_USERNAME=
WIPO_PASSWORD=
WIPO_LOGIN_URL=https://www.wipo.int/portal/en/
WIPO_SEARCH_URL=https://patentscope.wipo.int/search/pt/search.jsf
